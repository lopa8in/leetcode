    "def pref(strs):\n",
    "    if len(strts) == 0:\n",
    "        return ''\n",
    "\n",
    "    strs.sort()\n",
    "    first = strs[0]\n",
    "    end = strs[-1]\n",
    "\n",
    "    for i, prefs in enumerate(first):\n",
    "        if  i >= len(end) or prefs != end[i]:\n",
    "            return first[:i]"
