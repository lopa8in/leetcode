    "27. Удалить элемент"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "c1035620",
   "metadata": {},
   "outputs": [],
   "source": [
    "def del_element(nums, value):\n",
    "    k = len(nums)\n",
    "    i = 0\n",
    "\n",
    "    while i < len(nums):\n",
    "\n",
    "        if nums[i] == value:\n",
    "            nums.pop(i)\n",
    "            nums.append('_')\n",
    "            k -= 1\n",
    "        \n",
    "        else:\n",
    "            i +=1\n",
    "    \n",
    "    return nums,k"
   ]
  }
